<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <!-- Menú raíz -->
    <menuitem id="estate_menu_root"
              name="Inmobiliaria"
              sequence="10"/>

    <!-- Submenú -->
    <menuitem id="estate_property_menu"
              name="Propiedades"
              parent="estate_menu_root"
              sequence="10"/>

    <!-- Opción de menú que abre la acción -->
    <menuitem id="estate_property_menu_action"
              name="Gestionar Propiedades"
              parent="estate_property_menu"
              action="estate_property_action"
              sequence="10"/>

    <!-- Menú de configuración -->
    <menuitem id="estate_menu_settings"
          name="Configuración"
          parent="estate_menu_root"
          sequence="20"/>

    <!-- Opción de menú para tipos de propiedad -->
    <menuitem id="estate_property_type_menu"
          name="Tipos de propiedad"
          parent="estate_menu_settings"
          action="estate_property_type_action"
          sequence="10"/>

    <!-- Opción de menú para etiquetas de propiedad -->
    <menuitem id="estate_property_tag_menu"
          name="Etiquetas de propiedad"
          parent="estate_menu_settings"
          action="estate_property_tag_action"
          sequence="20"/>

</odoo>
